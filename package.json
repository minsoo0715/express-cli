{
  "name": "@minsoo0715/ex-cli",
  "version": "1.0.0",
  "description": "express template generating CLI project",
  "main": "./lib/index.js",
  "bin": {
    "ex-cli": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "sudo npm i -g && ex-cli",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "debug": "npm run create && npm run local"
  },
  "author": "whiteb",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.1",
    "clear": "^0.1.0",
    "commander": "^7.2.0",
    "figlet": "^1.5.0",
    "path": "^0.12.7",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minsoo0715/express-cli.git"
  },
  "keywords": [
    "typescript",
    "express",
    "nodejs",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/minsoo0715/express-cli/issues"
  },
  "homepage": "https://github.com/minsoo0715/express-cli#readme"
}
